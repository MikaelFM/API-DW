generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id @default(auto()) @map("_id") @db.ObjectId
  age      Int
  cpf      String  @unique
  name     String
  income   Float
  location String
  city     String  @default("")
}
